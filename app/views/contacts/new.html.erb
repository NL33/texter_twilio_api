<%= form_for (@contact) do |f| %>
  <div class="form-group">
    <%= f.label :name %></br> 
    <%= f.text_field :name%>
  </div>
  
   <div class="form-group">
    <%= f.label :number %></br>
    <%= f.text_field :number %>
  </div>

  <%= f.hidden_field :user_id, :value => current_user.id %>
  
  <%= f.submit(:class => "btn btn-primary") %>
<% end %>

<%= link_to "Home Page", root_path, :class => "btn btn-default" %>